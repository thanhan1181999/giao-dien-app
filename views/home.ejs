            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link " href="/bongda">
                        Bóng đá
                        <span class="sr-only">(current)</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/kinhdoanh">Kinh Doanh</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/thitruong">Thị trường</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/suckhoe">Sức Khỏe</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/hitech">Hi-tech</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/showbiz">Showbiz</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/thegioi">Thế giới</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/phaidep">Phái đẹp</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/amthuc">Ẩm thực</a>
                </li>
                <li>
                    <form action="" method="post">
                        <input type="text" name="word" placeholder="nhap tu cam tim..." autocomplete="off">
                        <input type="submit" value="search">
                    </form>
                </li>
            </ul>
        </div>
    </nav>
    <!-------------------->
    <section class="wthree-row py-5" style="background-color:greenyellow">
        <div class="container py-xl-5 py-lg-3">
            <ul class="demo row list-unstyled">
                <% 
                    results.forEach(function(rows){
                    %>
                    <li class="col-lg-6 col-md-6">
                        <div class="creatives-grid">

                            <a href="/<%=rows.type%>/<%=rows.id%>">
                                <!----->
                                <img src="<%=rows.image2%>" alt=" " class="img-fluid">
                            </a>

                            <div class="p-mask img-thumbnail">

                                <h4>
                                    <a href="/<%=rows.type%>/<%=rows.id%>">
                                        <%=rows.name%>
                                    </a>
                                </h4>

                                <span class="let mt-3"><%=rows.time%></span>

                            </div>
                        </div>
                    </li>
                    <%
                    }) 
                %>
            </ul>
        </div>
    </section>



    <br>
</body>
